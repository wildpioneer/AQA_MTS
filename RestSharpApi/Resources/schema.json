{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "announcement": {
      "type": "string"
    },
    "completed_on": {
      "type": "integer"
    },
    "default_role_id": {
      "type": "integer"
    },
    "default_role": {
      "type": "string"
    },
    "is_completed": {
      "type": "boolean"
    },
    "name": {
      "type": "string"
    },
    "show_announcement": {
      "type": "boolean"
    },
    "suite_mode": {
      "type": "integer"
    },
    "url": {
      "type": "string"
    },
    "users": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "id": {
              "type": "integer"
            },
            "global_role_id": {
              "type": "null"
            },
            "global_role": {
              "type": "null"
            },
            "project_role_id": {
              "type": "null"
            },
            "project_role": {
              "type": "null"
            }
          },
          "required": [
            "id",
            "global_role_id",
            "global_role",
            "project_role_id",
            "project_role"
          ]
        }
      ]
    },
    "groups": {
      "type": "array",
      "items": {}
    }
  },
  "required": [
    "id",
    "announcement",
    "completed_on",
    "default_role_id",
    "default_role",
    "is_completed",
    "name",
    "show_announcement",
    "suite_mode",
    "url",
    "users",
    "groups"
  ]
}